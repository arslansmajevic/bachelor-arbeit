        ]
    ] ;
    fhir:subject [
        fhir:reference pat:1 ;
        fhir:display "John Smith"
    ] ;
    fhir:effectiveDateTime "2023-04-15T10:00:00"^^xsd:dateTime ;
    fhir:result [
        fhir:reference obs:Obs1 ;
        fhir:display "CBC Complete Blood Count"
    ] .

