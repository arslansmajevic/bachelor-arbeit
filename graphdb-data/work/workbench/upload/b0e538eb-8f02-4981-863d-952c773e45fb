@prefix fhir: <http://hl7.org/fhir/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix : <http://example.org/fhir/CarePlan/> .
@prefix pat: <http://example.org/fhir/Patient/> .

:CarePlan1 a fhir:CarePlan ;
    fhir:nodeRole fhir:treeRoot ;
    fhir:status "active"^^xsd:string ;
    fhir:intent "plan"^^xsd:string ;
    fhir:subject [
        
        fhir:reference pat:1 ;
        fhir:display "John Smith"
    ] ;
    fhir:period [
        a fhir:Period ;
        fhir:start "2023-01-01"^^xsd:date ;
        fhir:end "2023-12-31"^^xsd:date
    ] ;
    fhir:goal [
        
        fhir:reference :Goal1 ;
        fhir:display "Manage hypertension"
    ] ;
    fhir:activity [
        a fhir:CarePlanActivity ;
        fhir:detail [
            a fhir:CarePlanActivityDetail ;
            fhir:status "scheduled"^^xsd:string ;
            fhir:code [
                a fhir:CodeableConcept ;
                fhir:coding [
                    a fhir:Coding ;
                    fhir:system "http://example.org/fhir/CodeSystem/activity-codes"^^xsd:string ;
                    fhir:code "exercise"^^xsd:string ;
                    fhir:display "Regular exercise"
                ]
            ] ;
            fhir:scheduledTiming [
                a fhir:Timing ;
                fhir:repeat [
                    a fhir:TimingRepeat ;
                    fhir:frequency 3 ;
                    fhir:period 1 ;
                    fhir:periodUnit "wk"
                ]
            ] ;
            fhir:description "30 minutes of moderate exercise, three times a week"
        ]
    ] .

