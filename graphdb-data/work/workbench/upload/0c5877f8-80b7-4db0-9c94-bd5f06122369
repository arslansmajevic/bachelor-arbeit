@prefix fhir: <http://hl7.org/fhir/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix : <http://example.org/fhir/Encounter/> .
@prefix pat: <http://example.org/fhir/Patient/> .

:Encounter1 a fhir:Encounter ;
    fhir:nodeRole fhir:treeRoot ;
    fhir:status "finished"^^xsd:string ;
    fhir:class [
        a fhir:Coding ;
        fhir:system "http://terminology.hl7.org/CodeSystem/v3-ActCode"^^xsd:string ;
        fhir:code "AMB"^^xsd:string ;
        fhir:display "Ambulatory"^^xsd:string
    ] ;
    fhir:type [
        a fhir:CodeableConcept ;
        fhir:coding [
            a fhir:Coding ;
            fhir:system "http://example.org/fhir/CodeSystem/encounter-type"^^xsd:string ;
            fhir:code "general"^^xsd:string ;
            fhir:display "General Consultation"^^xsd:string
        ]
    ] ;
    fhir:subject [
        fhir:reference pat:1 ;
        fhir:display "John A. Smith"
    ] ;
    fhir:period [
        a fhir:Period ;
        fhir:start "2023-04-01T09:00:00"^^xsd:dateTime ;
        fhir:end "2023-04-01T09:30:00"^^xsd:dateTime
    ] .

