            fhir:display "Confirmed"
        ]
    ] ;
    fhir:code [
        a fhir:CodeableConcept ;
        fhir:coding [
            a fhir:Coding ;
            fhir:system "http://snomed.info/sct"^^xsd:string ;
            fhir:code "38341003"^^xsd:string ;
            fhir:display "Hypertension"
        ]
    ] ;
    fhir:subject [
        fhir:reference pat:1 ;
        fhir:display "John Smith"
    ] ;
    fhir:recordedDate "2023-04-01"^^xsd:date .

